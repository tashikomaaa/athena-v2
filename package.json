{
	"name": "athena",
	"version": "4.3.4",
	"private": true,
	"scripts": {
		"start": "dotenv -e .env.development react-scripts start",
		"build": "dotenv -e .env.production react-scripts build",
		"serve": "serve -l 6001 -s build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint .",
		"lint:fix": "eslint --fix --ext .js --ext .ts --ext .jsx --ext .tsx ./src",
		"lint:scss": "stylelint **/*.scss",
		"lint:fix-scss": "stylelint --fix **/*.scss",
		"icon": "svgr SvgIcons -d src/components/icon/svg-icons --typescript",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"jest": {
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!pascalcase|prismjs|@omtanke/react-use-event-outside/.*)"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:storybook/recommended"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 4 version",
			">0.2%"
		]
	},
